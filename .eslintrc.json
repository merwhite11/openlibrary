{
  "root": true,
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
  },
  "rules": {
    "no-undef": "error",
    "no-extra-semi": "error",
    "eol-last": ["error", "always"],
    "no-mixed-spaces-and-tabs": "error",
    "no-useless-escape": "error",
    "no-unused-vars": "error"
  }
}
